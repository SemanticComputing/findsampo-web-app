    "facets": {
        "findText": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "sortByPredicate": "skos:prefLabel"
        },
        "findName": {
            "sortByPredicate": "ltk-s:find_name"
        },
        "findNumber": {
            "sortByPredicate": ":identifier"
        },
        "objectType": {
            "sortByPredicate": "OPTIONAL { ?id :object_type/skos:prefLabel ?orderBy . FILTER(LANG(?orderBy) = '<LANG>') }"
        },
        "province": {
            "sortByPredicate": ":found_in_province/skos:prefLabel"
        },
        "municipality": {
            "sortByPredicate": ":found_in_municipality/skos:prefLabel"
        },
        "earliestStartYear": {
            "orderByPattern": "OPTIONAL { ?id :has_creation_time_span/crm:P82a_begin_of_the_begin ?orderBy_ . BIND(xsd:date(?orderBy_) as ?orderBy) }"
        },
        "latestEndYear": {
            "orderByPattern": "OPTIONAL { ?id :has_creation_time_span/crm:P82b_end_of_the_end ?orderBy_ . BIND(xsd:date(?orderBy_) as ?orderBy) }"
        },
        "minThickness": {
            "sortByPredicate": ":min_thickness"
        },
        "maxThickness": {
            "sortByPredicate": ":max_thickness"
        },
        "diameter": {
            "sortByPredicate": ":diameter"
        },
        "facetObjectType": {
            "containerClass": "ten",
            "facetType": "hierarchical",
            "filterType": "uriFilter",
            "parentProperty": "skos:broader",
            "predicate": ":facet_object_type",
            "searchField": true
        },
        "material": {
            "containerClass": "ten",
            "facetType": "hierarchical",
            "filterType": "uriFilter",
            "parentProperty": "skos:broader",
            "predicate": ":material",
            "searchField": true,
            "sortByPredicate": ":material/skos:prefLabel"
        },
        "place": {
            "containerClass": "ten",
            "facetType": "hierarchical",
            "filterType": "uriFilter",
            "parentProperty": "skos:broader",
            "predicate": ":found_in_municipality/skos:exactMatch",
            "searchField": true,
            "sortByPredicate": ":found_in_municipality/skos:exactMatch/skos:prefLabel"
        },
        "dateTimespan": {
            "containerClass": "four",
            "endProperty": "crm:P82b_end_of_the_end",
            "facetType": "timespan",
            "filterType": "timespanFilter",
            "max": null,
            "min": null,
            "predicate": ":has_creation_time_span",
            "startProperty": "crm:P82a_begin_of_the_begin"
        },
        "period": {
            "containerClass": "ten",
            "facetType": "hierarchical",
            "filterType": "uriFilter",
            "parentProperty": "skos:broader",
            "predicate": ":period",
            "searchField": true,
            "sortByPredicate": ":period/skos:prefLabel"
        },
        "length": {
            "containerClass": "five",
            "facetType": "integer",
            "filterType": "integerFilterRange",
            "predicate": ":length",
            "sortByPredicate": ":length",
            "typecasting": "BIND(xsd:integer(ROUND(xsd:decimal(?value))) as ?valueAsInteger)"
        },
        "width": {
            "containerClass": "five",
            "facetType": "integer",
            "filterType": "integerFilterRange",
            "predicate": ":width",
            "sortByPredicate": ":width",
            "typecasting": "BIND(xsd:integer(ROUND(xsd:decimal(?value))) as ?valueAsInteger)"
