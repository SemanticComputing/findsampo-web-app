                        "tabPath": "nearby_finds",
                        "tabIcon": "AddLocation",
                        "sparqlQuery": "nearbyFindsQuery",
                        "resultMapper": "makeObjectList",
                        "instanceConfig": {
                            "createPopUpContent": "createPopUpContentFindSampo"
                        }
                    },
                    "findInstancePageRecommendations": {
                        "tabID": 3,
                        "component": "InstancePageTable",
                        "fetchResultsWhenMounted": true,
                        "tabPath": "recommendation_links",
                        "tabIcon": "Star",
                        "sparqlQuery": "similarFindsQuery",
                        "resultMapper": "makeObjectList",
                        "properties": [
                            {
                              "id": "similarObjectType",
                              "valueType": "object",
                              "makeLink": true,
                              "externalLink": false,
                              "sortValues": true,
                              "numberedList": false
                            },
                            {
                              "id": "similarMaterial",
                              "valueType": "object",
                              "makeLink": true,
                              "externalLink": false,
                              "sortValues": true,
                              "numberedList": false
                            },
                            {
                              "id": "similarPeriod",
                              "valueType": "object",
                              "makeLink": true,
                              "externalLink": false,
                              "sortValues": true,
                              "numberedList": false
                            },
                            {
                              "id": "similarExternalFind",
                              "valueType": "object",
                              "makeLink": true,
                              "externalLink": true,
                              "sortValues": true,
                              "numberedList": false
                            }
                          ]
                    }
                }
            }
        },
        "findsPlaces": {
            "tabID": 2,
