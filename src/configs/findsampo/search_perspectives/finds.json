        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesFinds.js",
    "baseURI": "http://ldf.fi/findsampo",
    "URITemplate": "<BASE_URI>/finds/<LOCAL_ID>",
    "facetClass": ":Find",
    "langTag": "fi",
    "frontPageImage": "findsampo/Museovirasto_063_vaaka_compressed.jpg",
    "searchMode": "faceted-search",
