                        "pageType": "instancePage",
                        "tabPath": "map",
                        "tabIcon": "AddLocation",
                        "sparqlQuery": "findInstancePageMapQuery",
                        "resultMapper": "makeObjectList",
                        "instanceConfig": {
                            "createPopUpContent": "createPopUpContentFindSampo"
                        }
                    },
                    "findInstancePageNearbyFinds": {
                        "tabID": 2,
                        "component": "LeafletMap",
