        "findText"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "finds": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "findPropertiesFacetResults",
                "pagesize": 10,
                "sortBy": "findNumber",
                "sortDirection": "asc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "findPropertiesInstancePage",
